require(colorout)
setOutputColors(
    index    = c(0, NA, 8),
    normal   = c(0, NA, 7),
    number   = c(0, NA, 2),
    negnum   = c(0, NA, 9),
    zero     = c(0, NA, 4),
    infinite = c(0, NA, 5),
    string   = c(0, NA, 15),
    date     = c(1, NA, 10),
    const    = c(1, NA, 2),
    true     = c(0, NA, 5),
    false    = c(0, NA, 13),
    warn     = c(0, NA, 14),
    stderror = c(0, NA, 14),
    error    = c(1, NA, 6),
    verbose  = FALSE
)
